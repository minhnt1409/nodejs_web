<div class="mt-4">
    <div class="row">
        {{#each rooms}}
        <div class="col-sm-10 col-lg-3">
            <div class="card">
                <div class="card-body">
                    <a href="/rooms/{{this.id_phong}}">
                        <h5 class="card-title">{{this.ten_phong}}</h5>               
                    </a>
                    <p class="card-text">{{this.loai_phong}}</p>
                    <p class="card-text">{{this.gia_phong}}</p>
                    <p class="card-text">{{this.trang_thai}}</p>
                </div>
            </div>
        </div>
        {{/each }}
    </div>
</div>
